{
  "name": "nuxt-app",
  "description": "Example project generated using the project-scaffolder and the nuxt app plugin",
  "license": "MIT",
  "private": true,
  "repository": "travi-test/nuxt-app",
  "bugs": "https://github.com/travi-test/nuxt-app/issues",
  "homepage": "https://github.com/travi-test/nuxt-app#readme",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "predeploy:netlify": "run-s build",
    "deploy:netlify": "npx --package netlify-cli netlify deploy --dir=./dist//",
    "lint:lockfile": "lockfile-lint --path package-lock.json --type npm --validate-https --allowed-hosts npm",
    "lint:js": "eslint . --cache",
    "lint:md": "remark . --frail",
    "generate:md": "remark . --output",
    "lint:sensitive": "ban",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "node ./dist//index.js",
    "export": "nuxt export",
    "serve": "nuxt serve",
    "clean": "rimraf ./dist/",
    "prebuild": "run-s clean",
    "test:unit": "nyc run-s test:unit:base",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "test:unit:base": "DEBUG=any jest --testPathPattern=src/.*\\.test\\.js$",
    "lint:peer": "npm ls >/dev/null",
    "prepare": "husky install"
  },
  "dependencies": {
    "nuxt": "2.15.8"
  },
  "devDependencies": {
    "@babel/register": "7.18.9",
    "@nuxtjs/eslint-module": "3.1.0",
    "@travi/any": "2.1.5",
    "@travi/babel-preset": "3.0.91",
    "@travi/eslint-config": "3.0.5",
    "@travi/eslint-config-jest": "1.0.66",
    "@vue/test-utils": "2.0.2",
    "ban-sensitive-files": "1.9.18",
    "codecov": "3.8.3",
    "commitlint-config-travi": "1.4.16",
    "cz-conventional-changelog": "3.3.0",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "jest-when": "3.5.1",
    "lockfile-lint": "4.8.0",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "remark-cli": "11.0.0",
    "remark-preset-lint-travi": "4.0.1",
    "remark-toc": "8.0.1",
    "rimraf": "3.0.2"
  }
}
